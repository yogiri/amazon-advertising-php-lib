  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "support": {
    "issues": "https://github.com/Blog-Ecommerce/amazon-advertising-php-lib/issues",
    "source": "https://github.com/Blog-Ecommerce/amazon-advertising-php-lib"
